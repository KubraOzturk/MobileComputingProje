<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kubra.instagramclone">
<uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <!--<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" android:maxSdkVersion="18" />-->
    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:allowBackup="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Utils.Home.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Utils.Share.ShareActivity"></activity>
        <activity android:name=".Utils.Search.SearchActivity"></activity>
        <activity android:name=".Utils.Likes.LikesActivity"></activity>
        <activity android:name=".Utils.Profile.ProfileActivity"></activity>
        <activity android:name=".Utils.Profile.AccountSettingsActivity"></activity>
        <activity android:name=".Utils.Login.LoginActivity"></activity>
        <activity android:name=".Utils.Login.RegisterActivity"></activity>
        <activity android:name=".Utils.Share.NextActivity"/>
    </application>

</manifest>